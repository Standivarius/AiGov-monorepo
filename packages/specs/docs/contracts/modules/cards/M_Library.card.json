{
  "schema_version": "0.1.0",
  "module_id": "M_Library",
  "summary": "Maintain the GDPR base scenario library and ensure scenarios conform to schema and taxonomy.",
  "boundaries": {
    "inputs": [
      {
        "name": "scenario_schema",
        "contract_ref_path": "packages/specs/scenarios/schemas/base_scenario_v1.schema.json"
      },
      {
        "name": "signals_taxonomy",
        "contract_ref_path": "packages/specs/docs/contracts/taxonomy/signals.json"
      }
    ],
    "outputs": [
      {
        "name": "scenario_library",
        "contract_ref_path": "packages/specs/scenarios/library/base/gdpr/CONVENTIONS.md"
      }
    ]
  },
  "knobs": [
    {
      "id": "library_root",
      "description": "Filesystem path to the GDPR base scenario library.",
      "value_type": "path",
      "required": true
    }
  ],
  "risks": [
    "Schema drift or taxonomy mismatch can invalidate scenarios."
  ]
}
