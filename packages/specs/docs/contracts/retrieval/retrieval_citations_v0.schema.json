{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Retrieval Citations v0",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "citations"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "const": "0.1.0"
    },
    "citations": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "evidence_artifact_id"
        ],
        "properties": {
          "evidence_artifact_id": {
            "type": "string"
          }
        }
      }
    }
  }
}
